{
  "name": "quasar-meteor",
  "version": "0.0.1",
  "description": "A Quasar + Meteor integration",
  "productName": "Quasar Meteor",
  "cordovaId": "org.cordova.quasar.app",
  "author": "Allan Alexandre <alexandesigner@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev -m spa",
    "build": "quasar build -m pwa",
    "bundler": "meteor-client bundle --destination node_modules/meteor.bundle.js --config meteor.bundle.config.json",
    "api": "cd api/ && meteor npm install && meteor run -p 4000",
    "lint": "eslint --ext .js,.vue src",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "@quasar/extras": "^1.3.3",
    "quasar": "^1.5.11"
  },
  "devDependencies": {
    "@quasar/app": "^1.4.3",
    "@vue/eslint-config-standard": "^5.0.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-vue": "^6.0.1",
    "strip-ansi": "^6.0.0"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "resolutions": {
    "ajv": "6.8.1"
  }
}
